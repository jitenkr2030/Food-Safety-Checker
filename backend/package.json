{
  "name": "foodsafe-ai-backend",
  "version": "1.0.0",
  "description": "Production-grade backend API for FoodSafe AI",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:subscription": "node src/migrations/migrate_subscription.js",
    "migrate:partnerships": "node src/migrations/migrate_partnerships.js",
    "seed": "knex seed:run",
    "build": "babel src -d dist",
    "docker:build": "docker build -t foodsafe-backend .",
    "docker:run": "docker run -p 3000:3000 foodsafe-backend"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.9.2",
    "knex": "^2.5.1",
    "pg": "^8.11.3",
    "redis": "^4.6.7",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.5",
    "tensorflow": "^4.10.0",
    "@tensorflow/tfjs-node": "^4.10.0",
    "aws-sdk": "^2.1450.0",
    "cloudinary": "^1.40.0",
    "winston": "^3.10.0",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "nodemailer": "^6.9.4",
    "axios": "^1.5.0",
    "uuid": "^9.0.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "csurf": "^1.11.0",
    "express-mongo-sanitize": "^2.2.0",
    "xss": "^1.0.14",
    "hpp": "^0.2.3",
    "express-slow-down": "^1.6.0",
    "razorpay": "^2.8.6",
    "stripe": "^12.5.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "eslint": "^8.47.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-promise": "^6.1.1",
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "babel-jest": "^29.6.4",
    "prettier": "^3.0.2",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "food-safety",
    "ai",
    "machine-learning",
    "api",
    "nodejs",
    "express"
  ],
  "author": "FoodSafe AI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/foodsafe-ai-backend.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/foodsafe-ai-backend/issues"
  },
  "homepage": "https://github.com/your-org/foodsafe-ai-backend#readme"
}
